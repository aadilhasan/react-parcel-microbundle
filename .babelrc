{
  "presets": ["@babel/preset-react", "@babel/preset-env"],
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    [
      "babel-plugin-module-resolver",
      {
        "root": ["./lib/"],
        "alias": {
          "utils": "./lib/utils",
          "const": "./lib/const"
        }
      }
    ]
  ]
}
