{
  "name": "react-parcel",
  "version": "0.0.1",
  "description": "react with parcel test",
  "source": "lib/components/index.js",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel public/index.html",
    "build": "microbundle -f esm --jsx React.createElement",
    "script": "node scripts/build.js"
  },
  "author": "aadil hasan",
  "license": "MIT",
  "dependencies": {
    "const": "^1.0.0",
    "microbundle": "^0.11.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "node-sass": "^4.13.1",
    "sass": "^1.25.0",
    "typescript": "^3.8.2"
  },
  "targets": {
    "module": {
      "isLibrary": true
    }
  }
}
